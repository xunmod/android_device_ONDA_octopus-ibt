<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <bool name="config_disableDirectBootBlock">true</bool>
    <bool name="config_delayStartComponent">true</bool>
    <!-- The default shortComponentName of Launcher which can be boot optimized   -->
    <string name="launcher_shortComponentName">com.android.launcher3/.Launcher</string>
    <!-- Whether to enable boot opt  -->
    <bool name="config_targetUseBootupOpz" translatable="false">true</bool>
</resources>
